{
  "check-coverage": true,
  "per-file": true,
  "lines": 99,
  "statements": 99,
  "functions": 98,
  "branches": 80,
  "include": ["lib/**/*ts"],
  "exclude": ["**/*.spec.ts", "test", "coverage", "dist/", "*.js", "**/*.d.ts"],
  "reporter": ["lcov", "text-summary", "html"],
  "cache": true,
  "all": true,
  "report-dir": "./coverage",
  "tmp-directory": ".nyc_output",
  "extension": [".ts"]
}
